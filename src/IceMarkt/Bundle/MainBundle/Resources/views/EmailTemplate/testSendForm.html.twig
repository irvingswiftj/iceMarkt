<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
    <h4 class="modal-title" id="myModalLabel">Test Send Email</h4>
</div>
{{ form_start(addMockRecipientForm) }}
<div class="modal-body">

    {{ form_errors(addMockRecipientForm) }}

    <div class="form-group">
        {{ form_row(addMockRecipientForm.first_name) }}
    </div>
    <div class="form-group">
        {{ form_row(addMockRecipientForm.last_name) }}
    </div>
    <div class="form-group">
        {{ form_row(addMockRecipientForm.email) }}
    </div>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
    {{ form_widget(addMockRecipientForm.send) }}
</div>
{{ form_end(addMockRecipientForm) }}